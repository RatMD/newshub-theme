{
    "name": "@rat.md/newshub-theme",
    "version": "1.3.0",
    "description": "A minimalist and responsive blog & magazine template with page builder and dark mode, designed with Bootstrap 5.2.",
    "keywords": [
        "template",
        "theme",
        "october",
        "octobercms"
    ],
    "main": "resources/js/theme.js",
    "scripts": {
        "build": "node esbuild.js",
        "watch": "node esbuild.js --watch",
        "build:js": "node esbuild.js --no-css",
        "watch:js": "node esbuild.js --no-css --watch",
        "build:css": "node esbuild.js --no-js",
        "watch:css": "node esbuild.js --no-js --watch",
        "vendors": "node esbuild.js --vendors"
    },
    "author": "rat.md <info@rat.md> (https://rat.md)",
    "license": "MIT",
    "private": true,
    "dependencies": {
        "@popperjs/core": "^2.11.6",
        "@rat.md/bs-lightbox": "^1.1.0",
        "bootstrap": "^5.3.0-alpha1",
        "bootstrap-icons": "^1.10.3",
        "keen-slider": "^6.8.5"
    },
    "devDependencies": {
        "esbuild": "^0.17.12",
        "esbuild-sass-plugin": "^2.8.0",
        "sass": "^1.59.3"
    }
}
